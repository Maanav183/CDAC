#include <iostream>
using namespace std;

int main() {
    string name;
	int marks;
    
    cout << "Enter the name of the student: " << endl;
    cin >> name;
    
	cout << "\nEnter student marks: " << endl;
    cin >> marks;

    if (marks >= 80) {
        cout << "\nGrade: A" << endl;
    } else if (marks >= 60 && marks < 80) {
        cout << "\nGrade: B" << endl;
    } else if (marks >= 40 && marks < 60) {
        cout << "\nGrade: C" << endl;
    } else {
        cout << "\nGrade: NA" << endl;
    }

    return 0;
}

